@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Brobnik</title>
    <meta name="robots" content="noindex, follow" />
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/assets/images/favicon.png">

    <!-- CSS
    ============================================ -->
    <!-- Icon Font CSS -->
    @Styles.Render("~/bundles/peicon7stroke/css")
    @Styles.Render("~/bundles/font-awesome/css")
    <!-- Plugins CSS -->
    @Styles.Render("~/bundles/animate/css")
    @Styles.Render("~/bundles/swiper-bundle.min/css")
    @Styles.Render("~/bundles/odometer/css")
    @Styles.Render("~/bundles/nice-select/css")
    @Styles.Render("~/bundles/select2/css")
    @Styles.Render("~/bundles/rangeSlider/css")
    @Styles.Render("~/bundles/style/css")

</head>

<body>
    <header>
        @Html.Partial("_Header")
    </header>
    @RenderBody()
    <footer>
        @Html.Partial("_Footer")
    </footer>

    <!-- JS
    ============================================ -->
    <!-- Modernizer & jQuery JS -->
    <script src="~/assets/js/vendor/modernizr-3.11.2.min.js"></script>
    <script src="~/assets/js/vendor/jquery-3.5.1.min.js"></script>

    <!-- Bootstrap JS -->
    <script src="~/assets/js/plugins/popper.min.js"></script>
    <script src="~/assets/js/plugins/bootstrap.min.js"></script>

    <!-- Plugins JS -->
    <script src="~/assets/js/plugins/swiper-bundle.min.js"></script>
    <script src="~/assets/js/plugins/ajax-contact.js"></script>
    <script src="~/assets/js/plugins/appear.js"></script>
    <script src="~/assets/js/plugins/odometer.min.js"></script>
    <script src="~/assets/js/plugins/jquery.nice-select.min.js"></script>
    <script src="~/assets/js/plugins/select2.min.js"></script>
    <script src="~/assets/js/plugins/ion.rangeSlider.min.js"></script>
    <script src="~/assets/js/plugins/jquery.zoom.min.js"></script>

    <!-- Main JS -->
    <script src="~/assets/js/main.js"></script>

</body>

</html>